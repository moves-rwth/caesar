coproc geo_invariant_useless(init_x: UInt) -> (cont: Bool, x: UInt)
    pre ?(true)
    post ?(!cont)
{
    x = init_x
    cont = true
    @invariant(?(cont))
    while cont {
        var prob_choice: Bool = flip(0.5)
        if prob_choice {
            cont = false
        } else {
            x = x + 1
        }
    }
}
