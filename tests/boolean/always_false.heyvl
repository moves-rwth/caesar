// RUN: @caesar verify --limited-functions variable-fuel --quantifier-instantiation e-matching @file
// CHECK: Verified

domain Dom {
    // Dummy function to test code generation for functions with nested quantifiers.
    // Just a complicated way of writing `false`.
    func alwaysFalse(n: UInt): Bool = forall i: UInt . alwaysFalse(i) != alwaysFalse(n)
}

proc main(n: UInt) -> () {
    assert ?(!alwaysFalse(n))
}